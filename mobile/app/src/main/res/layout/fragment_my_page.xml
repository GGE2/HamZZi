<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#fff2d6">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constparent1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="4dp"
            android:layout_marginRight="4dp"
            android:background="@drawable/title_new_a">
            <TextView
                android:id="@+id/textview_mypage_title"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="6dp"
                android:layout_marginTop="6dp"
                android:fontFamily="@font/mine"
                android:paddingStart="6dp"
                android:text="MYPAGE"
                android:textColor="#ffb100"
                android:textSize="50sp"
                android:textStyle="bold"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/const1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="4dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="4dp"
            android:background="@drawable/myinfo"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@id/constparent1">

            <ImageView
                android:id="@+id/imageview_my_page_icon_level"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="8dp"
                android:src="@drawable/lv_logo_4"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textview_my_page_content_level"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:fontFamily="@font/mine"
                android:text="HAMZZI"
                android:textColor="@color/black"
                android:textSize="30sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@id/imageview_my_page_icon_level"
                app:layout_constraintLeft_toRightOf="@id/imageview_my_page_icon_level"
                app:layout_constraintTop_toTopOf="@id/imageview_my_page_icon_level" />

            <ImageView
                android:id="@+id/imageview_my_page_icon_coin"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_marginLeft="18dp"
                android:layout_marginTop="8dp"
                android:src="@drawable/coin_1"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@id/imageview_my_page_icon_level" />

            <TextView
                android:id="@+id/textview_my_page_content_coin"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:fontFamily="@font/mine"
                android:text="9999"
                android:textColor="@color/black"
                android:textSize="30sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@id/imageview_my_page_icon_coin"
                app:layout_constraintLeft_toRightOf="@id/imageview_my_page_icon_coin"
                app:layout_constraintTop_toTopOf="@id/imageview_my_page_icon_coin" />

            <ImageView
                android:id="@+id/imageview_my_page_icon_place"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="14dp"
                android:src="@drawable/mygps_1"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@id/imageview_my_page_icon_coin"
                app:layout_constraintBottom_toBottomOf="parent"/>
            <TextView
                android:id="@+id/textview_my_page_content_place"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="등록된 장소가 없습니다"
                android:textSize="25sp"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:fontFamily="@font/mine"
                android:layout_marginLeft="16dp"
                app:layout_constraintTop_toTopOf="@id/imageview_my_page_icon_place"
                app:layout_constraintBottom_toBottomOf="@id/imageview_my_page_icon_place"
                app:layout_constraintLeft_toRightOf="@id/imageview_my_page_icon_place"/>
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constparent2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="4dp"
            android:layout_marginRight="4dp"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@id/const1"
            android:background="@drawable/title_new_a">

            <TextView
                android:id="@+id/textview_graduate_title"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="6dp"
                android:layout_marginTop="6dp"
                android:fontFamily="@font/mine"
                android:paddingStart="6dp"
                android:text="HISTORY"
                android:textColor="#ffb100"
                android:textSize="50sp"
                android:textStyle="bold"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constparent3"
            android:layout_width="match_parent"
            android:layout_height="280sp"
            app:layout_constraintTop_toBottomOf="@id/constparent2"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent">
            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="280sp">
                <ImageView
                    android:layout_width="180sp"
                    android:layout_height="match_parent"
                    android:src="@drawable/ham0"
                    android:layout_gravity="left"
                    />
                <ImageView
                    android:layout_width="180sp"
                    android:layout_height="match_parent"
                    android:src="@drawable/ham2"
                    android:layout_gravity="right"
                    />
            </androidx.appcompat.widget.LinearLayoutCompat>


            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="#e8e1d2"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"/>
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constparent4"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            app:layout_constraintTop_toBottomOf="@id/constparent3"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent">

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/button_mypage_regitser_place"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginLeft="20sp"
                android:layout_marginTop="20sp"
                android:background="@drawable/map"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"/>
            <TextView
                android:id="@+id/textview_mypage_regitser_place"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="장소 등록"
                android:fontFamily="@font/nexon_gothic_bold"
                android:textColor="@color/black"
                android:layout_marginTop="10sp"
                android:layout_marginLeft="14sp"
                app:layout_constraintTop_toBottomOf="@id/button_mypage_regitser_place"
                app:layout_constraintLeft_toLeftOf="parent"/>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/button_mypage_regitser_time"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginLeft="30sp"
                android:layout_marginTop="20sp"
                android:background="@drawable/clock"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toRightOf="@id/button_mypage_regitser_place"/>

            <TextView
                android:id="@+id/textview_mypage_register_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="시간 등록"
                android:fontFamily="@font/nexon_gothic_bold"
                android:textColor="@color/black"
                android:layout_marginTop="10sp"
                android:layout_marginLeft="20sp"
                app:layout_constraintTop_toBottomOf="@id/button_mypage_regitser_time"
                app:layout_constraintLeft_toRightOf="@id/textview_mypage_regitser_place"/>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/button_mypage_logout"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:background="@drawable/logout"
                android:layout_marginTop="20sp"
                android:layout_marginLeft="30sp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toRightOf="@id/button_mypage_regitser_time" />

            <TextView
                android:id="@+id/textview_mypage_logout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10sp"
                android:layout_marginLeft="20sp"
                android:text="로그아웃"
                android:fontFamily="@font/nexon_gothic_bold"
                android:textColor="@color/black"
                app:layout_constraintTop_toBottomOf="@id/button_mypage_logout"
                app:layout_constraintLeft_toRightOf="@id/textview_mypage_register_time"/>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/a"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:background="@drawable/ic_friend_add"
                android:layout_marginTop="20sp"
                android:layout_marginLeft="30sp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toRightOf="@id/button_mypage_logout" />
            <TextView
                android:id="@+id/a1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10sp"
                android:layout_marginLeft="20sp"
                android:text="친구추가"
                android:fontFamily="@font/nexon_gothic_bold"
                android:textColor="@color/black"
                app:layout_constraintTop_toBottomOf="@id/a"
                app:layout_constraintLeft_toRightOf="@id/textview_mypage_logout"/>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/b"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:background="@drawable/ic_guild"
                android:layout_marginTop="20sp"
                android:layout_marginLeft="30sp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toRightOf="@id/a" />
            <TextView
                android:id="@+id/b1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10sp"
                android:layout_marginLeft="20sp"
                android:text="길드정보"
                android:fontFamily="@font/nexon_gothic_bold"
                android:textColor="@color/black"
                app:layout_constraintTop_toBottomOf="@id/b"
                app:layout_constraintLeft_toRightOf="@id/a1"/>

        </androidx.constraintlayout.widget.ConstraintLayout>



    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>

